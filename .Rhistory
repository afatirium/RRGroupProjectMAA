### Plots for Summary Statistics
library(ggplot2)
library(gridExtra)
# Set the figure size
options(repr.plot.width = 12, repr.plot.height = 8)
# Plot 1: Histogram
plot1 <- ggplot(train, aes(x = SalePrice)) +
geom_histogram(bins = 50, alpha = 0.6, color = "black", fill = "lightblue") +
geom_vline(xintercept = mean(train$SalePrice), linetype = "dashed", color = "red", size = 1) +
geom_vline(xintercept = median(train$SalePrice), linetype = "dotted", color = "green", size = 1) +
labs(x = "Sale Price", y = "Counts") +
ggtitle("Sales Price Distribution") +
theme_bw()
# Plot 2: Scatter plot
plot2 <- ggplot(train, aes(x = SalePrice, y = rnorm(nrow(train), 7, 0.2))) +
geom_point(alpha = 0.5) +
ggtitle("Sales Price Distribution") +
theme_bw()
# Plot 3: Box plot
plot3 <- ggplot(train, aes(y = SalePrice)) +
geom_boxplot() +
labs(y = "Sale Price") +
ggtitle("Sales Price Distribution") +
theme_bw()
# Plot 4: Violin plot
plot4 <- ggplot(train, aes(y = SalePrice)) +
geom_violin(inner = "quartile", bw = 0.2) +
labs(y = "Sale Price") +
ggtitle("Sales Price Distribution") +
theme_bw()
# Combine all plots into a single figure
grid.arrange(plot1, plot2, plot3, plot4, ncol = 2)
View(plot1)
